        #self.onStopped() #activate the output of the box
        #self.speech.say("Hello, I have been trained in being a concierge, teller, and a chef. Which one of these roles would you like me to be?")
        conciergeArr = ['concierge', 'hotel', 'Hotel']
        tellerArr = ['tell our','tell are','Keller', 'Teller' ]
        chefArr = ['Club','Cocoa','Cook', 'Cooking', 'Chef Watson', 'chef' ]
        arrayOfRoles = [conciergeArr, tellerArr, chefArr]
        #s = self.sttStream()
        s = "Can you be a teller robot for me?"
        self.logger.info('The robot heard: {}'.format(s))

        index = self.findIndexOfJump(s, arrayOfRoles )
        self.logger.info(index)
        if index == 0:
            self.logger.info('Starting Concierge')
            self.startIndex0()
        if index == 1:
            self.logger.info('Starting Teller')
            self.startIndex1()
        if index == 2:
            self.logger.info('Starting Chef')
            self.startIndex2()
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def sttStream(self):
        # adding in beep at start
        self.logger.info( self.framemanager.getBehaviorPath(self.behaviorId) + "/Ding Sound Effect.mp3")
        self.player.post.playFileFromPosition(self.framemanager.getBehaviorPath(self.behaviorId) + "/../Ding Sound Effect.mp3", 0, .3, 0)

        # Catching cases from STT and looping untill a good transcription is heard
        while True:
            s = self.watson.stt_stream()
            self.logger.info("Hello")
            self.logger.info("SttStreamResult: {}".format(s))
            self.logger.info(type(s))
            s_json = json.loads(s)
            self.logger.info(type(s_json))
            if "state" in s_json.keys():
                pass
            elif "results" in s_json.keys():
                return str(s_json['results'][0]['alternatives'][0]['transcript'])
                break
    def findIndexOfJump(self, s, arrayOfClasses):
        for singleClass in arrayOfClasses:
            for test in singleClass:
                if test.lower() in s.lower():
                    return arrayOfClasses.index(singleClass)
        return -1
    def loadJsonFile(self, fileName):
        filePath = self.framemanager.getBehaviorPath(self.behaviorId) + fileName
        with open (filePath, "r") as myfile:
            data=myfile.read().replace('\n', '')
        return json.loads(data)
        #self.logger.info(data)
        #self.logger.info(json.loads(data))]]>
